{
	"manifest_version": 3,
	"name": "twxfilter",
	"version": "1.0.0",
	"description": "Media filter for Tw... X Web client.",
	"permissions": [
		"storage"
	],
	"host_permissions": [
		"<all_urls>"
	],
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"devtools_page": "devtools.html",
	"options_page": "options.html"
}